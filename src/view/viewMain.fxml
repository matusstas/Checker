<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="815.0" maxWidth="600.0" minHeight="815.0" minWidth="600.0" prefHeight="815.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerMain">
   <children>
      <TextArea fx:id="areaTestString" layoutX="31.0" layoutY="109.0" prefHeight="640.0" prefWidth="390.0" promptText="insert your test string here " wrapText="true" />
      <TextField fx:id="fieldRegularExpression" layoutX="31.0" layoutY="49.0" prefHeight="26.0" prefWidth="258.0" />
      <Label layoutX="31.0" layoutY="24.0" text="REGULAR EXPRESSION" />
      <Button layoutX="302.0" layoutY="49.0" mnemonicParsing="false" onAction="#check" prefHeight="26.0" prefWidth="86.0" text="C H E C K" />
      <Label layoutX="31.0" layoutY="85.0" text="TEST STRING" />
      <Label fx:id="labelStatus" layoutX="31.0" layoutY="761.0" />
      <Button layoutX="399.0" layoutY="49.0" mnemonicParsing="false" onAction="#help" text="?" />
      <RadioButton fx:id="radioSensitive" layoutX="431.0" layoutY="109.0" mnemonicParsing="false" selected="true" text="case sensitive">
         <toggleGroup>
            <ToggleGroup fx:id="groupCase" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="radioInsensitive" layoutX="431.0" layoutY="133.0" mnemonicParsing="false" text="case insensitive" toggleGroup="$groupCase" />
      <Button layoutX="431.0" layoutY="171.0" mnemonicParsing="false" onAction="#clean" text="C L E A N" />
   </children>
</AnchorPane>
